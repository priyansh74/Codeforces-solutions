#define KAMEHAMEHA            ios_base::sync_with_stdio(0);
#define RASENGAN              ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
using namespace std;
void solve()
{
	int n;
	cin>>n;
	int eights=ceil((double)n/4);
	for (int i = 0; i < (n-eights); i++)
	{
		cout<<9;
	}
	for (int i = 0; i < eights; i++)
	{
		cout<<8;
	}
	cout<<endl;
}
int main()
{
    KAMEHAMEHA
	int t=1;
	cin>>t;
	for(int cn=1;cn<=t;cn++)
	{
		solve();
	}
    return 0;
}

